<body>
  <div class="container">
    <div class="row">
      <div class="col-xs-9">
        <h2>ジャンル一覧/h2>

        

        <table class="table">
          <thead>
            <tr>
              <th>ジャンル</th>
              <th>状態</th>
              <th></th>
            </tr>
          </thead>
        <tbody>
        <% @genres.each do |genre| %>
          <tr>
            <td>
              <%= genre.name %>
            </td>
            <td>
              <%= genre.is_valid %>
            </td>
            <td>
              <%= link_to "編集する", edit_admin_genre_path(genre.id) %>
            </td>
          </tr>
        <% end %>
        </tbody>
        </table>
      </div>
    </div>
  </div>
</body>